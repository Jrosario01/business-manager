{
  "expo": {
    "name": "G & J Essence",
    "slug": "business-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "description": "Business management app for perfume inventory, sales, and shipment tracking",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a5490"
    },
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jrosario01.businessmanager",
      "infoPlist": {
        "NSContactsUsageDescription": "This app needs access to your contacts to help you quickly add customer information."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#1a5490"
      },
      "package": "com.jrosario01.businessmanager",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9f728ccd-a3c7-4408-a69e-4e992e3810ae"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
